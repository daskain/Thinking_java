import java.init.*;

class Task12 {
	public static void main (String[] args){
		
		int i = 0b11111111;
		i <<= 1;
		System.out.println(Integer.toBinaryString(i));
		for(; i != 1; i >>>= 1){
		System.out.println(Integer.toBinaryString(i));
		}
		System.out.println(Integer.toBinaryString(i));
    }
}

